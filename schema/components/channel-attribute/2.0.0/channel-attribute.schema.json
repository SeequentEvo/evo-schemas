{
  "$id": "/components/channel-attribute/2.0.0/channel-attribute.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "An attribute for a channel.",
  "type": "object",
  "properties": {
    "channel_reference": {
      "description": "A reference to the definition of the channel in the survey object header (the 'key' of the channel).",
      "type": "string"
    },
    "fiducial_start": {
      "description": "Starting fiducial number.",
      "type": "number"
    },
    "fiducial_increment": {
      "description": "Fiducial increment amount.",
      "type": "number",
      "exclusiveMinimum": 0.0
    },
    "values": {
      "description": "Values associated with the channel.",
      "type": "object",
      "oneOf": [
        {
          "$ref": "/elements/bool-array-1/1.0.1/bool-array-1.schema.json"
        },
        {
          "$ref": "/elements/float-array-1/1.0.1/float-array-1.schema.json"
        },
        {
          "$ref": "/elements/float-array-md/1.0.1/float-array-md.schema.json"
        },
        {
          "$ref": "/elements/integer-array-1/1.0.1/integer-array-1.schema.json"
        },
        {
          "$ref": "/elements/date-time-array/1.0.1/date-time-array.schema.json"
        },
        {
          "$ref": "/elements/string-array/1.0.1/string-array.schema.json"
        }
      ]
    }
  }
}